<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Montfort TT 35</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Montforttt35.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white">
            <div class="container-fluid">
                <div>
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img class="logo" src="~/logoMontfort.png" style="height: 172px;width: 283px;" /></a>
                    <h1 class="titre">MONTFORT T.T.</h1>
                </div>
                <div>
                    <a class="btn btn-primary" href="" role="button">Inscription</a>
                    <a class="btn btn-primary" href="" role="button">Connexion</a>
                </div>
            </div>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
        </nav>
        <nav class="mynavbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom">
            <div class="container-fluid">
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-center">
                    <ul class="nav">
                        <!--Club-->
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Le Club &#9662;</a>
                            <ul class="dropdown">
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Présentation</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Organigramme</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Actualités</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Interview</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Progressions</a>
                                </li>
                            </ul>
                        </li>
                        <!--Infos Pratiques-->
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Infos Pratiques &#9662;</a>
                            <ul class="dropdown">
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Section loisir</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Section compétiteur</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Entrainements</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Tarifs</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Partenaire</a>
                                </li>
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Liens utiles</a>
                                </li>
                            </ul>
                        </li>
                        <!--Compétitions-->
                        <li class="nav-item dropdown">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Compétitions &#9662;</a>
                            <ul class="dropdown">
                                <li class="nav">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Championnat par équipe &#9656;</a>
                                    <ul>
                                        <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Championnat adulte</a></li>
                                        <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Championnat jeune</a></li>
                                    </ul>
                                <li>
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Compétition individuels &#9656;</a>
                                    <ul>
                                        <li>
                                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Critérium &#9656;</a>
                                            <ul>
                                                <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Critérium féminin</a></li>
                                                <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Critérium fédéral</a></li>
                                            </ul>
                                        </li>
                                        <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Circuit des jeunes</a></li>
                                        <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Finales par classements</a></li>
                                        <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Championnat d’Ille-et-Vilaine</a></li>
                                    </ul>
                                </li>
                                <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Tournois</a></li>
                            </ul>
                        </li>
                        <!--Photos & Vidéos-->
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Photos & Vidéos</a>
                        </li>
                        <!--Contact & Plan-->
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Contact & Plan</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <p>1</p>
        <p>2</p>
        <p>3</p>
        <p>4</p>
        <p>5</p>
        <p>6</p>
        <p>7</p>
        <p>8</p>
        <p>9</p>
        <p>10</p>
        <p>11</p>
        <p>12</p>
        <p>13</p>
        <p>14</p>
        <p>15</p>
    </div>

    <footer class="border-top footer text-muted" style=" position:inherit; width: 100%; height: inherit; background: #FFFFFF; box-shadow: inset 0px 4px 4px rgba(0, 0, 0, 0.25);">
        <div class="container">
            &copy; 2022 Montfort T.T. - tous droits réservés
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
